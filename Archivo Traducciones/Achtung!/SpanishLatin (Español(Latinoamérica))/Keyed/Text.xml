	<!-- EN: Draft-Dependent -->
  <CommandMenuModeOptionAuto>Dependiente del Estado</CommandMenuModeOptionAuto>
	<!-- EN: The draft status of the colonist decides whether to show the context menu or move to the clicked location -->
  <CommandMenuModeOptionAutoExplained>El estado de alistamiento del colono decide si se muestra el menú contextual o se mueve a la ubicación clickeada</CommandMenuModeOptionAutoExplained>
	<!-- EN: Key for Menu -->
  <CommandMenuModeOptionPressForMenu>Tecla para Menú</CommandMenuModeOptionPressForMenu>
	<!-- EN: Hold the key to override the default action and display the context menu instead -->
  <CommandMenuModeOptionPressForMenuExplained>Mantén presionada la tecla para anular la acción predeterminada y mostrar el menú contextual</CommandMenuModeOptionPressForMenuExplained>
	<!-- EN: Key for Position -->
  <CommandMenuModeOptionPressForPosition>Tecla para Posición</CommandMenuModeOptionPressForPosition>
	<!-- EN: Hold the key to override the context menu and issue a positioning command -->
