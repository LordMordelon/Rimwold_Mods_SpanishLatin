  <VREH_BulletproofSkin.symbolPack.suffixSymbols.0.symbol>resistente</VREH_BulletproofSkin.symbolPack.suffixSymbols.0.symbol>
  <!-- EN: bulletproof -->
  <VREH_BulletproofSkin.symbolPack.wholeNameSymbols.0.symbol>blindado</VREH_BulletproofSkin.symbolPack.wholeNameSymbols.0.symbol>
  
  <!-- EN: luciferium dependency -->
  <VREH_ChemicalDependency_Luciferium.label>dependencia de luciferio</VREH_ChemicalDependency_Luciferium.label>
  <!-- EN: Carriers of this gene need to ingest Luciferium on a regular basis to survive. After 10 days without luciferium, carriers will suffer from luciferium deficiency. After 20 days, they will fall into a coma. After 30 days, they will die. -->
  <VREH_ChemicalDependency_Luciferium.description>Los portadores de este gen necesitan ingerir luciferio regularmente para sobrevivir. Después de 10 días sin luciferio, los portadores sufrirán deficiencia de luciferio. Después de 20 días, caerán en coma. Después de 30 días, morirán.</VREH_ChemicalDependency_Luciferium.description>
  <!-- EN: luciferium-dependent -->
  <VREH_ChemicalDependency_Luciferium.labelShortAdj>dependiente de luciferio</VREH_ChemicalDependency_Luciferium.labelShortAdj>
  <!-- EN: luci -->
  <VREH_ChemicalDependency_Luciferium.symbolPack.prefixSymbols.0.symbol>luci</VREH_ChemicalDependency_Luciferium.symbolPack.prefixSymbols.0.symbol>
  <!-- EN: ferium -->
