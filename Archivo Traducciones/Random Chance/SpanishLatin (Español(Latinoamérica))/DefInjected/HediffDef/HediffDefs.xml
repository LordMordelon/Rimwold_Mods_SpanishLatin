  <!-- EN: very shocked -->
  <RC_ElectricShockHediff.stages.very_shocked.label>muy conmocionado</RC_ElectricShockHediff.stages.very_shocked.label>
  <!-- EN: paralysed -->
  <RC_ElectricShockHediff.stages.paralysed.label>paralizado</RC_ElectricShockHediff.stages.paralysed.label>
  
